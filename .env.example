# User credentials for basic authentication
APP_USERNAME=admin
APP_PASSWORD=secret

# VAPID claims email for push notifications
# Should be a mailto: email address or an https:// URL
VAPID_CLAIMS_EMAIL=mailto:your-email@example.com

# Flask secret key for signing session cookies
# IMPORTANT: Change this to a long, random string
FLASK_SECRET_KEY=change-this-to-a-super-secret-random-string